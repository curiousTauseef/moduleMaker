<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.parseOptions.parseExtraOptions.controls.ParseExtraOptionsController">
   <children>
      <ButtonBar layoutY="411.0" prefHeight="48.0" prefWidth="820.0">
        <buttons>
            <Button mnemonicParsing="false" onAction="#handleBackAction" text="Back" />
            <Button mnemonicParsing="false" onAction="#handleContinueAction" text="Continue" />
        </buttons>
      </ButtonBar>
      <Text layoutX="14.0" layoutY="66.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Which files or folders should exist?" />
      <VBox fx:id="fileAndFolders" layoutX="82.0" layoutY="83.0" />
      <Text layoutX="316.0" layoutY="97.0" strokeType="OUTSIDE" strokeWidth="0.0" text="call of short options:" />
      <Text layoutX="316.0" layoutY="127.0" strokeType="OUTSIDE" strokeWidth="0.0" text="call of long options:" />
      <Text layoutX="298.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="call options:" />
      <TextField fx:id="textShort" layoutX="436.0" layoutY="79.0" text="no short options" />
      <TextField fx:id="textLong" layoutX="436.0" layoutY="108.0" text="no long options" />
      <Text layoutX="14.0" layoutY="208.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Return type elements:" />
      <TableView fx:id="returnTypeElementTable" layoutX="14.0" layoutY="224.0" prefHeight="223.0" prefWidth="223.0">
        <columns>
          <TableColumn prefWidth="128.0" text="Name" >
          <cellValueFactory>
						<PropertyValueFactory property="name" />
					</cellValueFactory>
					</TableColumn>
          <TableColumn prefWidth="94.0" text="Type" >
           <cellValueFactory>
						<PropertyValueFactory property="type" />
					</cellValueFactory>
					</TableColumn>
        </columns>
      </TableView>
      <TextField fx:id="nameReturnTypeElement" layoutX="272.0" layoutY="242.0" prefHeight="26.0" prefWidth="185.0" promptText="Name of returnTypeelement" />
      <ComboBox fx:id="typeReturnTypeElement" layoutX="468.0" layoutY="242.0" prefHeight="26.0" prefWidth="97.0">
       <items>
        <FXCollections fx:factory="observableArrayList">
          <String fx:value="string" />
          <String fx:value="integer" />
          <String fx:value="boolean" />
          <String fx:value="AbsoluteFilePath" />
          <String fx:value="AbsoluteFolderPath" />
        </FXCollections>
		</items>
      </ComboBox>
      <Button layoutX="572.0" layoutY="242.0" mnemonicParsing="false" onAction="#handleAddNewReturnTypeAction" text="Add returnTypeElement" />
      <Button layoutX="272.0" layoutY="290.0" mnemonicParsing="false" onAction="#handleRemoveSelectedReturnTypeElementAction" text="Remove selected returnTypeElement" />
   </children>
</AnchorPane>
